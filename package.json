    "@emotion/cache": "^11.13.1",
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.13.0",
    "@emotion/server": "^11.11.0",
    "h3": "^0.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/serve-static": "^1.13.10",
    "esno": "^0.14.1",
    "parcel": "^2.4.1",
    "process": "^0.11.10",
